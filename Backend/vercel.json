{
    "version": 2,
    "name": "amlak-react",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "app.js"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/app.js"
        }
    ]
}